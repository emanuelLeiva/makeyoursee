<section id="services" class="text-gray-600 body-font">
  <div class="container px-5 lg:py-24 mx-auto">
    <h1
      class="text-4xl font-bold text-center mt-11 title-font text-black tracking-widest bg-white bg-opacity-70"
    >
      Servicios
    </h1>
